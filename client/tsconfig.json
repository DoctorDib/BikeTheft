{
    "compilerOptions": {
      "outDir": "./public/",
      "noImplicitAny": true,
      "module": "es6",
      "target": "es6",
      "jsx": "react",
      "allowJs": true,
      "sourceMap": true,
      "allowSyntheticDefaultImports" : true,
      "esModuleInterop" : true,
      "lib": [
        "es6", 
        "dom",
        "dom.iterable",
      ],
      "noImplicitThis": true,
      "strictNullChecks": true,
      "strict": true,
      "moduleResolution": "node",
      "types" : ["material-ui"],
      "isolatedModules": true,
      "resolveJsonModule": true
    }
}